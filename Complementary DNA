function DNAStrand(dna){

  let sArr = dna.split("")
  
  for(let i = 0; i < sArr.length; i++){
    if(sArr[i] === 'A'){
      sArr[i] = "T";
      }
    else if(sArr[i] === 'T'){
      sArr[i] = "A";
      }
     if(sArr[i] === 'C'){
      sArr[i] = "G";
      }
   
  else if(sArr[i] === 'G'){
      sArr[i] = "C";
      }
  }
  
  return sArr.join("");
  
}
