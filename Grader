function grader(score) {
  let answer;
  
  if(score > 1 || score < 0.6){
    answer = "F"
  }
  
  if(score >= 0.9 && score <= 1){
    answer = "A"
  }
  
  if(score >= 0.8 && score < 0.9){
    answer = "B"
  }
  
   if(score >= 0.7 && score < 0.8){
    answer = "C"
  }
  
   if(score >= 0.6 && score < 0.7){
    answer = "D"
  }
return answer;
}
