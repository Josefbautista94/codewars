function turn(current, target) {
  let answer = ''
  if(current === "N" && target === "E" || current === 'S' && target === "W"|| current === "E" && target === "S" || current === "W"&& target === "N"){
    answer = 'right'
  }
   if(current === "N" && target === "W" || current === "S" && target === "E" || current === 'E'&& target === "N" || current === "W" && target === "S"){
    answer = 'left'
  }
  return answer;
}
