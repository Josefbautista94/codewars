function DNAtoRNA(dna) {
  
  let splitString = dna.split('');
  let answer = []
  
  for(let i = 0; i < splitString.length; i++){
    if(splitString[i] === 'G'){answer.push('G')}
        if(splitString[i] === 'C' ){answer.push('C')}
            if(splitString[i] === 'A'){answer.push('A')}
                if(splitString[i] === 'T' ){answer.push('U')}
  }
  
  return answer.join('');
}
