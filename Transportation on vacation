function rentalCarCost(d) {
  // Your solution here
  
  // Every day yo rent the car its going to cost you 40 bucks
  // If you rent the car for 3 or more days you get 20 off you total 
  // If you rent the car for 7 days you get 50 off your total
  
  
if(d < 3){
   return 40 * d;

}
  if(d < 7){
     return 40 * d - 20;
    
  }
  
  if(d >= 7){
    return 40 * d -50;
  }
  
}
